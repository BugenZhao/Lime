var version = || {
    println("Lime 0.1.0");
};

var max = |a, b| {
    if a > b { a; } else { b; }
};
var min = |a, b| {
    if a < b { a; } else { b; }
};

var id = |x| { x; };

var assert_eq = |a, b| {
    if a !== b { panic("Assertion failed:\n  left:", dbg(a), "\n right:", dbg(b)); }
    true;
};
